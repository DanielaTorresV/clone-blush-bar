        "props": {
          "width": "grow",
          "verticalAlign": "middle"
        }
      },
      "link#container__about--link": {
        "children": [
          "rich-text#container__about--text"
        ],
        "props": {
          "href": "https://blushbardanitorres--itgloberspartnercl.myvtex.com/que-es-blush-bar"
